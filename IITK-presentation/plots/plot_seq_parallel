#!/usr/bin/gnuplot


set terminal postscript eps enhanced color 30
set style fill solid

set xlabel "Algorithm"
set xlabel ""
set ylabel "Execution time (in sec)"
set boxwidth 0.45
set key below
set style line 1 linewidth 4
set size ratio 0.56
set ytic 0.25
#set yrange [0:1]
#set xtics ("A" 1, "B" 2) scale 0.0


#set tics font "Helvetica,30"
#set output 'single-core-performance-seq-parallel-rsvd.eps'
set output 'single-core-performance-seq-svd-rsvd-parallel-rsvd.eps'
plot 'tt-rsvd.data' u 1:3:xtic(2)  w boxes lt rgb "green" t "", '' using 1:($3 + 0.075):3 with labels t ""

