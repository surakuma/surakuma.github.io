#!/usr/bin/gnuplot


set terminal postscript eps enhanced color 32
set style fill solid

set xlabel "Algorithm"
set ylabel "Execution time (in sec)"
set boxwidth 0.45
set key below
set style line 1 linewidth 4
set yrange [0:2.5]
#set xtics ("A" 1, "B" 2) scale 0.0

set output 'single-core-performance-seq-svd-rsvd.eps'
plot 'seq.data' u 1:3:xtic(2)  w boxes lt rgb "green" t "", '' using 1:($3+0.1):3 with labels t ""

